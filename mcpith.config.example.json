{
  "downstream": {
    "transport": "stdio"
  },
  "upstreams": [
    {
      "id": "filesystem",
      "name": "Filesystem Server",
      "transport": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/tmp"],
      "enabled": true
    },
    {
        "id": "fetch",
        "name": "Fetch",
        "transport": "stdio",
        "command": "uvx",
        "args": ["mcp-server-fetch"],
        "enabled": true
    }
  ],
  "compression": {
    "baseUrl": "http://localhost:8080/v1",
    "model": "your-model",
    "defaultPolicy": {
      "enabled": true,
      "tokenThreshold": 1000,
      "maxOutputTokens": 500
    },
    "toolPolicies": {
      "filesystem__read_file": {
        "enabled": false
      },
      "fetch__fetch": {
        "tokenThreshold": 500,
        "maxOutputTokens": 200,
        "customInstructions": "Preserve all URLs, dates, and code examples verbatim."
      }
    }
  },
  "cache": {
    "enabled": true,
    "ttlSeconds": 300,
    "maxEntries": 1000
  },
  "tools": {
    "hidden": [
      "everything__echo",
      "everything__longRunningOperation"
    ]
  },
  "logLevel": "info"
}
